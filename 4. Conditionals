x = 5


if x == 0:
    print('zero true')
elif x== 1:
    print('one 1 true')
elif x== 2:
    print('elif 2 true')
elif x == 3:
    print('elif 3 true')
elif x == 4:
    print('elif 4 true')
elif x == 5:
    print('elif 5 true')
elif False:
    print('elif 4 true')
else:
    print('neither true')

